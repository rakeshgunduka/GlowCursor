{
    "name": "Glow CurSor",
    "version": "1.0",
    "description": "Playable Mouse Cursor",
    "homepage_url": "",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Glow CurSor"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
    ],
}
